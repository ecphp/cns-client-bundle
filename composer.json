{
    "name": "ecphp/cns-client",
    "type": "symfony-bundle",
    "description": "Corporate Notification Service Client",
    "keywords": [
        "CNS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">= 7.4",
        "ecphp/php-conventions": "dev-master",
        "psr/http-client": "^1.0",
        "symfony/http-foundation": "^5.3",
        "symfony/monolog-bundle": "^3.7",
        "ext-json": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "EcPhp\\CnsClientBundle\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
